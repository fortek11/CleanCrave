<div class="container-fluid container-fluid-custom">
    <h1>Book Your Slot Now!</h1>
        
        <form action="/tracker" enctype="multipart/form-data" method="post">
          <div class="form-group">
            <label for="startDate" class = "form-label">Slot Date:</label>
            <input type="date" class="form-control" id="startDate" name="startDate" required>
          </div>
    
          <div class="form-group">
            <label for="cycleLength" class = "form-label">Patient Name:</label>
            <input type="number" class="form-control" id="cycleLength" name="cycleLength" required>
          </div>
    
          <div class="form-group">
            <label for="periodLength" class = "form-label">Email:</label>
            <input type="number" class="form-control" id="periodLength" name="periodLength" required>
          </div>
    
          <div class="form-group">
            <label for="symptoms" class = "form-label">Symptoms:</label>
            <textarea class="form-control" id="symptoms" name="symptoms"></textarea>
          </div>
    
          <div class="form-group">
            <label for="reminder" class = "form-label">Any previous Medical History</label>
            <textarea class="form-control" id="reminder" name="reminder" rows="5"></textarea>
          </div>
    
          <button type="submit" class="btn btn-dark" style="margin-top: 30px;">Submit</button>
        </form>
</div>
